{
    "name": "@totalpave/regression",
    "description": "Javascript least squares data fitting methods",
    "contributors": [
        {
            "name": "Norman Breau",
            "email": "norman.breau@totalpave.com"
        }
    ],
    "publishConfig": {
        "registry": "https://registry.totalpave.com:48731"
    },
    "version": "5.0.0",
    "license": "MIT",
    "main": "./lib/api.js",
    "typings": "./lib/api.d.ts",
    "homepage": "https://github.com/totalpave/regression-js#readme",
    "repository": "totalpave/regression-js",
    "keywords": [
        "regression",
        "data",
        "fiting",
        "modeling",
        "analysis"
    ],
    "nyc": {
        "check-coverage": true,
        "per-file": true,
        "include": "src/**/*.ts",
        "extension": [
            ".ts"
        ]
    },
    "scripts": {
        "clean": "git clean -fdx -e node_modules",
        "lint": "eslint ./src ./spec",
        "lint:fix": "eslint --fix ./src ./spec",
        "test": "npm run lint && jest",
        "build": "echo \"Typescript `tsc -v`\" && tsc && echo \"Build successful.\"",
        "build-docs": "typedoc --excludePrivate --readme ./README.md --out ./docs ./src/api.ts",
        "prepublish": "npm run clean && npm run build && npm test",
        "changelog": "auto-changelog --output CHANGELOG.md -p --release-summary --ignore-commit-pattern chore --issue-pattern '[A-Z]+-\\d+' --issue-url https://totalpave.atlassian.net/browse/{id}",
        "version": "npm run build-docs && npm run changelog && git add CHANGELOG.md docs && git commit -m 'chore: release'",
        "postversion": "git push && git push --tags"
    },
    "peerDependencies": {
        "@totalpave/interfaces": "^5.0.0",
        "@arashi/token": "^1.0.0"
    },
    "devDependencies": {
        "@totalpave/eslint-plugin": "7.0.16",
        "@types/jest": "30.0.0",
        "auto-changelog": "2.5.0",
        "jest": "30.0.5",
        "jest-jasmine2": "30.0.5",
        "ts-jest": "29.4.1",
        "ts-node": "10.9.2",
        "typedoc": "0.28.10",
        "typedoc-plugin-markdown": "4.8.1",
        "typescript": "5.9.2",
        "@types/node": "24.3.0",
        "@totalpave/interfaces": "5.2.0",
        "@arashi/token": "1.0.1"
    },
    "dependencies": {
        "@totalpave/math": "2.3.0",
        "@totalpave/object": "4.0.0",
        "tslib": "2.8.1"
    }
}
